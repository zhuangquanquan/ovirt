<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:d="urn:import:org.ovirt.engine.ui.common.widget.dialog"
             xmlns:e="urn:import:org.ovirt.engine.ui.common.widget.editor"
             xmlns:t="urn:import:org.ovirt.engine.ui.common.widget.dialog.tab"
             xmlns:ge="urn:import:org.ovirt.engine.ui.common.widget.editor.generic"
             xmlns:w="urn:import:org.ovirt.engine.ui.common.widget"
             xmlns:k="urn:import:org.ovirt.engine.ui.common.widget.form.key_value"
             xmlns:vm="urn:import:org.ovirt.engine.ui.common.widget.uicommon.popup.vm"
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:l="urn:import:org.ovirt.engine.ui.common.widget.label">

    <ui:with field='resources' type='org.ovirt.engine.ui.webadmin.ApplicationResources' />
    <ui:with field='constants' type='org.ovirt.engine.ui.webadmin.ApplicationConstants' />
    <ui:with field='templates' type='org.ovirt.engine.ui.webadmin.ApplicationTemplates' />

    <ui:style type="org.ovirt.engine.ui.webadmin.section.main.view.popup.cluster.ClusterPopupView.WidgetStyle">
        .generalTabTopDecorator {
            background-color: #D3D3D3;
        }

        .explanationLabel {
            font-style: italic;
            margin-left: 40px;
            margin-bottom: 10px;
            width: 300px;
        }

        .label {
            width: 100%;
        }

        .messageLabel {
            color: #FF0000;
            left: 10px;
            padding-left: 5px;
            padding-top: 10px;
        }

        .explanationLabel {
            font-style: italic;
            margin: 10px 5px;
        }

        .fingerprintLabel textarea {
            height: 35px;
        }

        .editorContentWidget {
            width: 350px;
            margin-top: 10px;
        }

        .smallPanelTitle, .panelTitle {
            padding-left: 3px;
            padding-bottom: 10px;
            display: inline-block;
        }

        .transparent {
            width: 400px;
            height: 40px;
            opacity: .8;
        }

        .geryPanel {
            background-color: grey;
            height: 20px;
            width: 160px;
        }

        .minLabel {
            height: 10px;
            width: 10px;
            margin-top: 3px;
            margin-left: 6px;
            background-color: #AFBF27;
        }

        .maxLabel {
            height: 10px;
            width: 10px;
            margin-top: 3px;
            margin-left: 6px;
            background-color: #4E9FDD;
        }

        .timeTextBoxEditorWidget {
            width: 30px;
            padding: 0px;
            margin: 0px;
            vertical-align: top;
            height: 20px;
            line-height: 20px;
        }

        .labelStyle {
            margin-top: 3px;
        }

        .labelStyle {
            font-weight: bold;
            margin-top: 10px;
            margin-bottom: 5px;
        }

        .optimizationTabPanel {
            height: 470px;
        }

        .sectionPanel {
            margin-bottom: 20px;
        }

        .additionalFeaturesExpanderContent {
            margin-left: 15px;
            display: none;
        }

        .container {
            width: auto;
        }

        .floatLeft {
            float: left
        }

        .topElement {
            padding-top: 15px;
        }

        .groupTitle {
            font-weight: bold;
            padding-top: 10px;
            padding-bottom: 5px;
        }

        /* patternfly defines it as !important and it needs to be bold so overridding it */
        .labelTitle {
            font-weight: bold !important;
        }

        .firstRow {
            padding-top: 15px;
        }

        .narrowGutters {
            margin-left: -4px;
            margin-right: -4px;
        }

        /* http://arnique.net/web-design/58/a-quick-guide-to-changing-bootstraps-gutter-width/ */
        @external .col-sm-6;

        .narrowGutters .col-sm-6 {
            padding-left: 4px;
            padding-right: 4px;
        }

        .panelInfo {
            display: inline-block;
            margin-left: 5px;
        }

        .keyValueWidget {
            padding: 0 5px;
        }

        .infoIcon {
            float: left;
            padding-left: 2px;
        }

        .checkBoxLabel {
            padding-left: 10px;
            padding-right: 10px;
            position: relative;
        }

        .patternFlycheckBoxLabel {
            padding-left: 10px;
            padding-right: 10px;
            position: relative;
        }
    </ui:style>

    <d:SimpleDialogPanel width="800px" height="560px">
        <d:content>
            <t:DialogTabPanel ui:field="tabPanel" height="100%">
                <t:tab>
                    <t:DialogTab ui:field="generalTab" label="{constants.clusterPopupGeneralTabLabel}">
                        <t:content>
                            <b:Container addStyleNames="{style.container}">
                                <b:Row ui:field="dataCenterRow" addStyleNames="{style.firstRow} {style.generalTabTopDecorator}">
                                    <e:ListModelListBoxEditor ui:field="dataCenterEditor" label="{constants.clusterPopupDataCenterLabel}" usePatternFly="true" labelColSize="LG_6" widgetColSize="LG_6" />
                                </b:Row>
                                <b:Row addStyleNames="{style.firstRow}">
                                    <ge:StringEntityModelTextBoxEditor ui:field="nameEditor" label="{constants.clusterPopupNameLabel}" usePatternFly="true" labelColSize="LG_6" widgetColSize="LG_6" />
                                </b:Row>
                                <b:Row>
                                    <ge:StringEntityModelTextBoxEditor ui:field="descriptionEditor" label="{constants.descriptionLabel}" usePatternFly="true" labelColSize="LG_6" widgetColSize="LG_6" />
                                </b:Row>
                                <b:Row>
                                    <ge:StringEntityModelTextBoxEditor ui:field="commentEditor" label="{constants.commentLabel}" usePatternFly="true" labelColSize="LG_6" widgetColSize="LG_6" />
                                </b:Row>
                                <b:Row>
                                    <e:ListModelListBoxEditor ui:field="managementNetworkEditor" label="{constants.managementNetworkLabel}" usePatternFly="true" labelColSize="LG_6" widgetColSize="LG_6" />
                                </b:Row>
                                <b:Row>
                                    <e:ListModelListBoxEditor ui:field="architectureEditor" label="{constants.clusterPopupArchitectureLabel}" usePatternFly="true" labelColSize="LG_6" widgetColSize="LG_6" />
                                </b:Row>
                                <b:Row>
                                    <e:ListModelListBoxEditor ui:field="cpuEditor" label="{constants.clusterPopupCPUTypeLabel}" usePatternFly="true" labelColSize="LG_6" widgetColSize="LG_6" />
                                </b:Row>
                                <b:Row>
                                    <e:ListModelListBoxEditor ui:field="versionEditor" label="{constants.clusterPopupVersionLabel}" usePatternFly="true" labelColSize="LG_6" widgetColSize="LG_6" />
                                </b:Row>
                                <b:Row>
                                    <e:ListModelListBoxEditor ui:field="switchTypeEditor" label="{constants.clusterSwitchTypeLabel}" usePatternFly="true" labelColSize="LG_6" widgetColSize="LG_6" />
                                </b:Row>
                                <b:Row>
                                    <b:Column size="LG_12">
                                        <ge:EntityModelCheckBoxEditor ui:field="enableOvirtServiceEditor" label="{constants.clusterEnableOvirtServiceLabel}" removeFormGroup="true" usePatternFly="true" />
                                        <ge:EntityModelCheckBoxEditor ui:field="enableGlusterServiceEditor" label="{constants.clusterEnableGlusterServiceLabel}" removeFormGroup="true" usePatternFly="true" />
                                        <ge:EntityModelRadioButtonEditor ui:field="enableOvirtServiceOptionEditor" label="{constants.clusterEnableOvirtServiceLabel}" removeFormGroup="true" usePatternFly="true" />
                                        <ge:EntityModelRadioButtonEditor ui:field="enableGlusterServiceOptionEditor" label="{constants.clusterEnableGlusterServiceLabel}" removeFormGroup="true" usePatternFly="true" />
                                    </b:Column>
                                </b:Row>
                                <b:Row>
                                    <b:Column size="LG_12">
                                        <ge:EntityModelCheckBoxEditor ui:field="importGlusterConfigurationEditor" label="{constants.clusterImportGlusterConfigurationLabel}" removeFormGroup="true" usePatternFly="true" />
                                    </b:Column>
                                </b:Row>
                                <b:Row>
                                    <g:Label ui:field="importGlusterExplanationLabel" addStyleNames="{style.explanationLabel}" text="{constants.clusterImportGlusterConfigurationExplanationLabel}" />
                                    <ge:StringEntityModelTextBoxEditor ui:field="glusterHostAddressEditor" label="{constants.hostPopupHostAddressLabel}" usePatternFly="true" labelColSize="LG_6" widgetColSize="LG_6"/>
                                </b:Row>
                                <b:Row>
                                    <ge:StringEntityModelTextAreaLabelEditor addStyleNames="{style.fingerprintLabel}" ui:field="glusterHostFingerprintEditor" label="{constants.hostPopupHostFingerprintLabel}" usePatternFly="true" labelColSize="LG_6" widgetColSize="LG_6"/>
                                </b:Row>
                                <b:Row>
                                    <ge:StringEntityModelPasswordBoxEditor ui:field="glusterHostPasswordEditor" label="{constants.hostPopupPasswordLabel}" usePatternFly="true" labelColSize="LG_6" widgetColSize="LG_6" />
                                </b:Row>
                                <b:Row>
                                    <b:Column size="LG_12">
                                        <g:Label ui:field="messageLabel" addStyleNames="{style.messageLabel}" />
                                        <ge:EntityModelCheckBoxEditor ui:field="enableOptionalReasonEditor" label="{constants.clusterPolicyEnableReasonLabel}" removeFormGroup="true" usePatternFly="true" />
                                        <ge:EntityModelCheckBoxEditor ui:field="enableHostMaintenanceReasonEditor" label="{constants.clusterPolicyEnableHostMaintenanceReasonLabel}" removeFormGroup="true" usePatternFly="true" />
                                    </b:Column>
                                </b:Row>
                                <b:Row>
                                    <b:Column size="LG_12">
                                        <g:Label ui:field="rngLabel"  addStyleNames="{style.smallPanelTitle}" text="{constants.requiredRngSources}" />
                                        <ge:EntityModelCheckBoxEditor ui:field="rngRandomSourceRequired" label="{constants.rngSourceRandom}" removeFormGroup="true" usePatternFly="true" />
                                        <ge:EntityModelCheckBoxEditor ui:field="rngHwrngSourceRequired" label="{constants.rngSourceHwrng}" removeFormGroup="true" usePatternFly="true" />
                                    </b:Column>
                                </b:Row>
                                <b:Row>
                                    <e:ListModelListBoxEditor ui:field="glusterTunedProfileEditor" label="{constants.glusterTunedProfileLabel}" usePatternFly="true" labelColSize="LG_6" widgetColSize="LG_6" />
                                </b:Row>
                                <b:Row>
                                    <b:Column size="LG_12">
                                        <d:AdvancedParametersExpander ui:field="additionalFeaturesExpander" titleWhenCollapsed="{constants.addtionalClusterFeaturesTitle}" titleWhenExpanded="{constants.addtionalClusterFeaturesTitle}" />
                                        <g:FlowPanel ui:field="additionalFeaturesExpanderContent">
                                             <e:ListModelCheckBoxGroup ui:field="additionalFeaturesEditor" />
                                        </g:FlowPanel>
                                    </b:Column>
                                </b:Row>
                            </b:Container>
                        </t:content>
                    </t:DialogTab>
                </t:tab>
                <t:tab>
                    <t:DialogTab ui:field="optimizationTab" label="{constants.clusterPopupOptimizationTabLabel}">
                        <t:content>
                            <b:Container addStyleNames="{style.container}">
                                <b:Row addStyleNames="{style.firstRow}">
                                    <b:Column size="LG_12">
                                        <g:Label ui:field="memoryOptimizationPanelTitle" addStyleNames="{style.floatLeft}" text="{constants.clusterPopupMemoryOptimizationPanelTitle}" />
                                        <d:InfoIcon ui:field="memoryOptimizationInfo" addStyleNames="{style.panelInfo}" />
                                        <ge:EntityModelRadioButtonEditor ui:field="optimizationNoneEditor" label="{constants.clusterPopupOptimizationNoneLabel}" removeFormGroup="true" usePatternFly="true" />
                                        <ge:EntityModelRadioButtonEditor ui:field="optimizationForServerEditor" removeFormGroup="true" usePatternFly="true" />
                                        <ge:EntityModelRadioButtonEditor ui:field="optimizationForDesktopEditor" removeFormGroup="true" usePatternFly="true" />
                                        <ge:EntityModelRadioButtonEditor ui:field="optimizationCustomEditor" removeFormGroup="true" usePatternFly="true" />
                                    </b:Column>
                                </b:Row>
                                <b:Row ui:field="cpuThreadsRow">
                                    <b:Column size="LG_12">
                                        <g:Label ui:field="cpuThreadsPanelTitle" addStyleNames="{style.floatLeft}" text="{constants.clusterPopupCpuThreadsPanelTitle}" />
                                        <d:InfoIcon ui:field="cpuThreadsInfo" addStyleNames="{style.panelInfo}" />
                                        <ge:EntityModelCheckBoxEditor ui:field="countThreadsAsCoresEditor" label="{constants.clusterPopupCountThreadsAsCoresLabel}" removeFormGroup="true" usePatternFly="true" />
                                    </b:Column>
                                </b:Row>
                                <b:Row>
                                    <b:Column size="LG_12">
                                        <g:Label text="{constants.ballooningLabelTitle}" />
                                        <ge:EntityModelCheckBoxEditor ui:field="enableBallooning" label="{constants.enableBallooningLabel}" removeFormGroup="true" usePatternFly="true" />
                                    </b:Column>
                                </b:Row>
                                <b:Row>
                                    <b:Column size="LG_12">
                                        <g:Label text="{constants.ksmLabelTitle}" />
                                        <ge:EntityModelCheckBoxEditor ui:field="enableKsm" label="{constants.enableKsmLabel}" removeFormGroup="true" usePatternFly="true" />
                                        <e:ListModelRadioGroupEditor ui:field="ksmPolicyForNumaEditor" usePatternFly="true" />
                                    </b:Column>
                                </b:Row>
                            </b:Container>
                        </t:content>
                    </t:DialogTab>
                </t:tab>
                <t:tab>
                    <t:DialogTab ui:field="migrationTab" label="{constants.clusterPopupMigrationTabLabel}">
                        <t:content>
                            <b:Container addStyleNames="{style.container}">
                                <b:Row addStyleNames="{style.groupTitle} {style.firstRow}">
                                    <b:Column size="LG_12">
                                        <g:Label text="{constants.clusterMigrationPolicyLabel}" />
                                    </b:Column>
                                </b:Row>
                                <b:Row>
                                    <b:Column size="LG_12">
                                        <e:ListModelListBoxEditor ui:field="migrationPolicyEditor" addStyleNames="{style.floatLeft}" />
                                        <g:HTML ui:field="migrationPolicyDetails" />
                                    </b:Column>
                                </b:Row>
                                <b:Row addStyleNames="{style.groupTitle}">
                                    <b:Column size="LG_12">
                                        <g:Label text="{constants.clusterPopupBandwidthLabel}" />
                                    </b:Column>
                                </b:Row>
                                <b:Row>
                                    <b:Column size="LG_6" >
                                        <l:LabelWithTooltip text="{constants.clusterPopupMigrationBandwidthLimit}" forWidget="{migrationBandwidthLimitTypeEditor}" />
                                        <d:InfoIcon text="{templates.migrationBandwidthLimit}" addStyleNames="{style.panelInfo}" tooltipPlacement="AUTO"/>
                                    </b:Column>
                                    <b:Column size="LG_6" >
                                        <b:Row addStyleNames="{style.narrowGutters}">
                                            <b:Column size="SM_6" >
                                                <ge:BootstrapListBoxListModelEditor ui:field="migrationBandwidthLimitTypeEditor" />
                                            </b:Column>
                                            <b:Column size="SM_6" >
                                                <ge:IntegerEntityModelEditor ui:field="customMigrationBandwidthLimitEditor" />
                                            </b:Column>
                                        </b:Row>
                                    </b:Column>
                                </b:Row>
                                <b:Row addStyleNames="{style.groupTitle}">
                                    <b:Column size="LG_12">
                                        <l:LabelWithTooltip text="{constants.clusterPopupResiliencePolicyLabel}" addStyleNames="{style.labelTitle}"/>
                                        <d:InfoIcon text="{templates.clusterPopupResiliencePolicyInfo}" addStyleNames="{style.panelInfo}" />
                                    </b:Column>
                                </b:Row>
                                <b:Row>
                                    <b:Column size="LG_12">
                                        <ge:EntityModelRadioButtonEditor ui:field="migrateOnErrorOption_YESEditor" addStyleNames="{style.label}" label="{constants.clusterPopupMigrateOnError_YesLabel}" removeFormGroup="true" usePatternFly="true" />
                                        <ge:EntityModelRadioButtonEditor ui:field="migrateOnErrorOption_HA_ONLYEditor" addStyleNames="{style.label}" label="{constants.clusterPopupMigrateOnError_HaLabel}" removeFormGroup="true" usePatternFly="true" />
                                        <ge:EntityModelRadioButtonEditor ui:field="migrateOnErrorOption_NOEditor" addStyleNames="{style.label}" label="{constants.clusterPopupMigrateOnError_NoLabel}" removeFormGroup="true" usePatternFly="true" />
                                    </b:Column>
                                </b:Row>
                                <b:Row addStyleNames="{style.groupTitle}">
                                    <b:Column size="LG_12">
                                        <g:Label text="{constants.clusterAdditionalMigrationProperties}" />
                                    </b:Column>
                                </b:Row>

                                <b:Row>
                                    <b:Column size="LG_12">
                                        <e:ListModelListBoxEditor ui:field="autoConvergeEditor" label="{constants.autoConvergeLabel}" usePatternFly="true" labelColSize="LG_6" widgetColSize="LG_6" />
                                    </b:Column>
                                </b:Row>
                                <b:Row>
                                    <b:Column size="LG_12">
                                        <e:ListModelListBoxEditor ui:field="migrateCompressedEditor" label="{constants.migrateCompressedLabel}" usePatternFly="true" labelColSize="LG_6" widgetColSize="LG_6" />
                                    </b:Column>
                                </b:Row>
                            </b:Container>
                        </t:content>
                    </t:DialogTab>
                </t:tab>
                <t:tab>
                    <t:DialogTab ui:field="clusterPolicyTab" label="{constants.clusterPopupClusterPolicyTabLabel}">
                        <t:content>
                            <b:Container addStyleNames="{style.container}">
                                <b:Row addStyleNames="{style.firstRow}">
                                    <e:ListModelListBoxEditor ui:field="clusterPolicyEditor" label="{constants.clusterPolicySelectPolicyLabel}" usePatternFly="true" labelColSize="LG_6" widgetColSize="LG_6" />
                                </b:Row>
                                <b:Row>
                                    <b:Column size="LG_12">
                                        <g:Label addStyleNames="{style.labelStyle}" text="{constants.clusterPolicyPropertiesLabel}" />
                                        <k:KeyValueWidget ui:field="customPropertiesSheetEditor" addStyleNames="{style.keyValueWidget}" usePatternFly="true"/>
                                    </b:Column>
                                </b:Row>
                                <b:Row>
                                    <b:Column size="LG_12">
                                        <g:Label ui:field="schedulerOptimizationPanelTitle" addStyleNames="{style.floatLeft}" text="{constants.schedulerOptimizationPanelLabel}" />
                                        <d:InfoIcon ui:field="schedulerOptimizationInfoIcon" addStyleNames="{style.panelInfo}" />
                                    </b:Column>
                                </b:Row>
                                <b:Row>
                                    <b:Column size="LG_12">
                                        <ge:EntityModelRadioButtonEditor ui:field="optimizeForUtilizationEditor" removeFormGroup="true" usePatternFly="true" label="{constants.optimizeForUtilizationLabel}" />
                                        <ge:EntityModelRadioButtonEditor ui:field="optimizeForSpeedEditor" removeFormGroup="true" usePatternFly="true" label="{constants.optimizeForSpeedLabel}" />
                                    </b:Column>
                                </b:Row>
                                <b:Row ui:field="allowOverbookingRow">
                                    <b:Column size="LG_12">
                                        <ge:EntityModelRadioButtonEditor ui:field="guarantyResourcesEditor" removeFormGroup="true" usePatternFly="true" label="{constants.guarantyResourcesLabel}" />
                                        <ge:EntityModelRadioButtonEditor ui:field="allowOverbookingEditor" removeFormGroup="true" usePatternFly="true" label="{constants.allowOverbookingLabel}" />
                                        <d:InfoIcon ui:field="allowOverbookingInfoIcon" addStyleNames="{style.panelInfo}" />
                                    </b:Column>
                                </b:Row>
                                <b:Row>
                                    <b:Column size="LG_12">
                                        <g:Label ui:field="additionPropsPanelTitle" text="{constants.clusterPolicyAdditionalPropsPanelTitle}" />
                                        <ge:EntityModelCheckBoxEditor ui:field="enableTrustedServiceEditor" removeFormGroup="true" usePatternFly="true" label="{constants.clusterPolicyEnableTrustedServiceLabel}" />
                                        <ge:EntityModelCheckBoxEditor ui:field="enableHaReservationEditor" removeFormGroup="true" usePatternFly="true" label="{constants.clusterPolicyEnableHaReservationLabel}" />
                                    </b:Column>
                                </b:Row>
                                <b:Row>
                                    <b:Column size="LG_12">
                                        <vm:SerialNumberPolicyWidget ui:field="serialNumberPolicyEditor" usePatternFly="true" />
                                    </b:Column>
                                </b:Row>
                            </b:Container>
                        </t:content>
                    </t:DialogTab>
                </t:tab>
                <t:tab>
                    <t:DialogTab ui:field="consoleTab" label="{constants.consoleTabLabel}">
                        <t:content>
                          <b:Container addStyleNames="{style.container}">
                            <b:Row addStyleNames="{style.topElement}">
                              <b:Column size="LG_6">
                                <ge:EntityModelCheckBoxEditor ui:field="spiceProxyOverrideEnabled" addStyleNames="{style.floatLeft}" usePatternFly="true" removeFormGroup="true" />
                                <g:Label text="{constants.overriddenSpiceProxyAddress}" addStyleNames="{style.floatLeft} {style.patternFlycheckBoxLabel}" />
                                <d:InfoIcon ui:field="isVirtioScsiEnabledInfoIcon" addStyleNames="{style.infoIcon}"/>
                              </b:Column>
                              <ge:StringEntityModelTextBoxEditor ui:field="spiceProxyEditor" usePatternFly="true" widgetColSize="LG_6" />
                            </b:Row>
                          </b:Container>
                        </t:content>
                    </t:DialogTab>
                </t:tab>
                <t:tab>
                    <t:DialogTab ui:field="fencingPolicyTab" label="{constants.fencingPolicyTabLabel}">
                        <t:content>
                          <b:Container addStyleNames="{style.container}">
                            <b:Row addStyleNames="{style.topElement}">
                              <b:Column size="LG_12">
                                <ge:EntityModelCheckBoxEditor ui:field="fencingEnabledCheckBox" removeFormGroup="true" usePatternFly="true" addStyleNames="{style.floatLeft}" label="{constants.fencingEnabled}" />
                                <d:InfoIcon ui:field="fencingEnabledInfo" addStyleNames="{style.panelInfo}" />
                              </b:Column>
                            </b:Row>
                            <b:Row>
                               <b:Column size="LG_12">
                                <ge:EntityModelCheckBoxEditor ui:field="skipFencingIfSDActiveCheckBox" removeFormGroup="true" usePatternFly="true" addStyleNames="{style.floatLeft}" label="{constants.skipFencingIfSDActive}" />
                                <d:InfoIcon ui:field="skipFencingIfSDActiveInfo" addStyleNames="{style.panelInfo}" />
                              </b:Column>
                            </b:Row>
                            <b:Row>
                              <b:Column size="LG_7">
                                <ge:EntityModelCheckBoxEditor ui:field="skipFencingIfConnectivityBrokenCheckBox" usePatternFly="true" addStyleNames="{style.floatLeft}" />
                                <g:Label ui:field="skipFencingIfConnectivityBrokenCheckBoxLabel" addStyleNames="{style.floatLeft} {style.checkBoxLabel}" text="{constants.skipFencingWhenConnectivityBroken}" />
                                <d:InfoIcon ui:field="skipFencingIfConnectivityBrokenInfo" addStyleNames="{style.floatLeft}" />
                              </b:Column>
                              <b:Column size="LG_5">
                                <e:ListModelListBoxEditor ui:field="hostsWithBrokenConnectivityThresholdEditor" usePatternFly="true" labelColSize="LG_5" widgetColSize="LG_7" addStyleNames="{style.floatLeft}" label="{constants.hostsWithBrokenConnectivityThresholdLabel}" />
                              </b:Column>
                            </b:Row>
                          </b:Container>
                        </t:content>
                    </t:DialogTab>
                </t:tab>
            </t:DialogTabPanel>
        </d:content>
    </d:SimpleDialogPanel>
</ui:UiBinder>
